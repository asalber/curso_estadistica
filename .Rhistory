if (coeffs1[1] < 0 ) plusorminus <- c("")
text(0, 8.5, paste("$y=", coeffs1[2], "x", plusorminus, coeffs1[1], "$"), pos = 4)
text(0, 7.7, paste("$r=", round(cor(hours1, score1), 2), "$"), pos = 4)
